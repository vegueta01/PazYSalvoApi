                {{!-- <li class="list-group-item">Ciudad: {{city}}</li> --}}
                <li class="list-group-item text-center p-1 card-header text-center">Quien entrega</li>
                <li class="list-group-item font-weight-bold text-center" 
                style="padding-top: 3rem; padding-bottom: 1px;">{{employeeName}}</li>
                {{#if employeeCharge}}
                    <li class="list-group-item text-center p-1">{{employeeCharge}}</li>
                {{else}}
                    <li class="list-group-item text-center p-1">Empleado</li>
                {{/if}}
            </ul>
        </div>

        <div class="card mt-3" style="width: 22rem; float:right ;">
            <ul class="list-group list-group-flush">
                {{!-- <li class="list-group-item">Ciudad: {{city}}</li> --}}
