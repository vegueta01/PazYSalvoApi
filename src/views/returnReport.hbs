    <div class="card mt-2" >
        {{#if tableTitle}}
        <h5 class="card-header text-center" style="padding-top: 5px; padding-bottom: 5px;">{{tableTitle}}</h5>
        {{/if}}
        <div class="card-body" style="padding-top: 2px; padding-bottom: 1px;">
            {{!-- <h5 class="card-title">Equipos</h5> --}}
            <table class="table" style="padding-top: 2px; padding-bottom: 2px;">
                <thead >
                    <tr >
                        {{!-- <th scope="col" style="padding-top: 3px; padding-bottom: 3px;">#</th> --}}
                        {{!-- {{#each array}}
                            {{#if @index}} --}}
                                {{#each fields}}
                                    <th scope="col" style="padding-top: 3px; padding-bottom: 3px;">{{this}}</th>
                                {{/each}}
                            {{!-- {{/if}}
                        {{/each}} --}}
                        {{!-- <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th> --}}
                    </tr>
                </thead>
                <tbody>
                    {{#each array}}
                    <tr>
                        {{!-- <th scope="row" style="padding-top: 2px; padding-bottom: 2px;">{{@index}}</th> --}}
                        {{#each this}}
                        <td style="padding-top: 2px; padding-bottom: 2px;">{{this}}</td>
                        {{/each}}
                        {{!-- <td>{{this.nombre}}</td>
            <td>{{this.apellido}}</td>
            <td>mdo</td> --}}
                    </tr>
                    {{/each}}
